{
  "name": "vongola12324/vCTF",
  "description": "A simple CTF system implement by Laravel.",
  "version": "0.0.0",
  "keywords": [
    "CTF"
  ],
  "authors": [
    {
      "name": "Vongola",
      "email": "vongola12324@coder.tw",
      "homepage": "https://vongola.tw",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/vongola12324/vctf/issues",
    "wiki": "https://github.com/vongola12324/vCTF/wiki"
  },
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "arcanedev/log-viewer": "^4.5",
    "barryvdh/laravel-ide-helper": "^2.4",
    "bepsvpt/secure-headers": "^4.2",
    "bnbwebexpertise/laravel-attachments": "^1.0",
    "doctrine/dbal": "^2.6",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "5.6.*",
    "laravel/tinker": "^1.0",
    "lavary/laravel-menu": "^1.7",
    "predis/predis": "^1.1",
    "recca0120/laravel-tracy": "^1.8",
    "santigarcor/laratrust": "^5.0",
    "spatie/laravel-backup": "^5.4",
    "spatie/laravel-html": "^2.19",
    "thomaswelton/laravel-gravatar": "^1.1",
    "yish/generators": "^2.0"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "bootstrap/constants.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "@php artisan clear-compiled",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
